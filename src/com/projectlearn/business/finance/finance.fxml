<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="finance" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.projectlearn.business.finance.FinanceController">
   <children>
      <Label fx:id="account" layoutX="14.0" layoutY="27.0" prefHeight="37.0" prefWidth="112.0" />
      <Button fx:id="deposit" layoutX="14.0" layoutY="561.0" mnemonicParsing="false" onAction="#deposit" prefHeight="26.0" prefWidth="71.0" text="Deposit" />
      <TextField fx:id="amount" layoutX="97.0" layoutY="561.0" prefHeight="26.0" prefWidth="71.0" promptText="amount" />
      <TextField fx:id="accountNum" layoutX="326.0" layoutY="33.0" onAction="#welcome" promptText="Enter account number" />
      <Label fx:id="balance" layoutX="709.0" layoutY="562.0" prefHeight="25.0" prefWidth="53.0" />
      <Button fx:id="withdraw" layoutX="191.0" layoutY="562.0" mnemonicParsing="false" onAction="#withdraw" text="Withdraw" />
      <TextField fx:id="withdrawAmount" layoutX="272.0" layoutY="562.0" prefHeight="25.0" prefWidth="71.0" promptText="amount" />
   </children>
</AnchorPane>
